<!DOCTYPE html>
<html>
<head>
  <title>ehip</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <!-- START: Facebook Open Graph Tags -->
	<meta property="og:title" content="<%= og_title(yield(:og_title)) %>" />
	<meta property="og:description" content="<%= og_desc(yield(:og_desc)) %>" />
	<meta property="og:type" content=<%= og_type(yield(:og_type)) %> />
	<meta property="og:image" content=<%= og_image(yield(:og_image)) %> />
	<meta property="og:url" content=<%= og_url(yield(:og_url)) %> />
	<meta property="og:site_name" content="ehip"/>
	<meta property="fb:admins" content="81001713"/>
	<meta property="fb:app_id" content="260308270754625"/>  
  <!-- END: Facebook Open Graph Tags -->  
  <%= render 'layouts/shim' %>   
	<%= render 'layouts/javascript' %>
</head>
<body>
	<%= render 'layouts/header' %>
	<div class="container">
		<%= yield %>
	</div>
		<%= render 'layouts/footer' %>
		<%= debug(params) if Rails.env.development? %>	
</body>
</html>